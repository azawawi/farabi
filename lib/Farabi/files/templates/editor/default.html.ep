<!DOCTYPE html>
<html>
<head>
<title>Farabi: Modern Perl Editor</title>

<!-- The jQuery -->
<script src="assets/jquery/jquery.1.9.0.min.js"></script>

<!-- Bootstrap -->
<link href="assets/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
<script src="assets/bootstrap/js/bootstrap.min.js"></script>

<!-- Farabi -->
<link rel="stylesheet" href="farabi.css" type="text/css" />
<script src="farabi.js"></script>
<script src="farabi_actions_dialog.js"></script>
<script src="farabi_open_file_dialog.js"></script>
<script src="farabi_repl_dialog.js"></script>
<script src="farabi_run_dialog.js"></script>
<script src="farabi_perl_critic_dialog.js"></script>
<script src="farabi_options_dialog.js"></script>
<script src="farabi_save_question_dialog.js"></script>
<script src="farabi_duplicate_perl_code_dialog.js"></script>
<script src="farabi_plugin_manager_dialog.js"></script>

<!-- CodeMirror editor -->
<link rel="stylesheet" href="assets/codemirror/lib/codemirror.css"
	type="text/css" />
<script src="assets/codemirror/lib/codemirror.js"></script>

<!-- CodeMirror addons -->
<link rel="stylesheet" href="assets/codemirror/addon/dialog/dialog.css" />
<script src="assets/codemirror/addon/mode/loadmode.js"></script>
<script src="assets/codemirror/addon/search/searchcursor.js"></script>
<script src="assets/codemirror/addon/search/match-highlighter.js"></script>
<script src="assets/codemirror/addon/search/search.js"></script>
<script src="assets/codemirror/addon/dialog/dialog.js"></script>

</head>
<body>
	<div class="container-fluid">
		<div class="row-fluid">
			<div>
				<br/>
				&nbsp;&nbsp;&nbsp;&nbsp;<a href="#" class="actions-button"><i class="icon-th"></i> <b>A</b>ctions</a>
				<br/>
				<br/>
			</div>
			<div class="row-fluid">
				<ul id="myTab" class="nav nav-tabs"></ul>
				<div id="myTabContent" class="tab-content"></div>
			</div>
			<div class="row-fluid">
				<b>Output:</b>
				<textarea id="output" cols="80" rows="10"></textarea>
			</div>
		</div>
	</div>

<%= include "editor/actions_dialog"%>
<%= include "editor/perl_critic_dialog"%>
<%= include "editor/about_dialog"%>
<%= include "editor/help_dialog"%>
<%= include "editor/run_dialog"%>
<%= include "editor/options_dialog"%>
<%= include "editor/open_file_dialog"%>
<%= include "editor/perl_doc_dialog"%>
<%= include "editor/repl_dialog"%>
<%= include "editor/save_question_dialog"%>
<%= include "editor/duplicate_perl_code_dialog"%>
<%= include "editor/plugin_manager_dialog"%>

</body>
</html>
